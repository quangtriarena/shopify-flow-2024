		"shopify": "shopify",
		"develop": "gulp && shopify app dev",
		"serve_shopify": "shopify app serve",
		"generate": "shopify app generate extension",
		"app_config_push": "shopify app config push",
		"deploy": "shopify app deploy"
	},
