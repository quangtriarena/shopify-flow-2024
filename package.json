{
	"name": "shopify-flow-app",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"client": "cd client && npm run dev",
		"server": "cd server && npm run dev",
		"client:prod": "cd client && npm run build",
		"serve": "cd server && npm run serve",
		"dev": "concurrently \"npm run client\" \"npm run server\"",
