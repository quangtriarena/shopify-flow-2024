		"@shopify/cli": "^3.68.0",
		"@shopify/ui-extensions": "2024.10.0",
		"@shopify/ui-extensions-react": "2024.10.0",
		"dotenv": "^16.4.5",
		"gulp": "^5.0.0"
	}
